{
  "name": "nforce",
  "description": "nforce is a REST API wrapper for force.com, database.com, and salesforce.com",
  "version": "1.12.1",
  "author": {
    "name": "Kevin O'Hara",
    "email": "kevnohara80@gmail.com",
    "url": "http://kevohara.com"
  },
  "keywords": [
    "salesforce",
    "salesforce.com",
    "force.com",
    "lightning",
    "sfdc",
    "forcedotcom",
    "rest",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kevinohara80/nforce.git"
  },
  "main": "dist/index.js",
  "dependencies": {
    "faye": "^1.2.4",
    "lodash": "^4.17.15",
    "mime": "^2.4.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.10",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.44",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typedoc": "^0.15.4",
    "typescript": "^3.4.4"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">= 0.12"
  },
  "bugs": {
    "url": "http://github.com/kevinohara80/nforce/issues"
  },
  "scripts": {
    "clean": "rm -rf dist docs",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.pkg.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "mocha './test/**/*.ts'",
    "docs": "rm -rf docs && typedoc --options typedoc.json src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "license": "MIT"
}
