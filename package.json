{
  "name": "nforce",
  "description": "nforce is a REST API wrapper for force.com, database.com, and salesforce.com",
  "version": "1.12.1",
  "author": {
    "name": "Kevin O'Hara",
    "email": "kevnohara80@gmail.com",
    "url": "http://kevinmohara.com"
  },
  "keywords": [
    "salesforce",
    "salesforce.com",
    "force.com",
    "database.com",
    "sfdc",
    "forcedotcom",
    "rest",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kevinohara80/nforce.git"
  },
  "main": "dist/index.js",
  "dependencies": {
    "faye": "^1.2.4",
    "lodash": "^4.17.11",
    "mime": "^2.4.2",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.10",
    "@types/request-promise": "^4.1.44",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "prettier": "^1.17.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">= 0.12"
  },
  "bugs": {
    "url": "http://github.com/kevinohara80/nforce/issues"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "mocha --require ts-node/register ./test/**/*.ts --reporter spec",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "license": "MIT"
}
